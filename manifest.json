{
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "images/logo.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_page_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Alt+Shift+P"
      }
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "App will filter product card item",
  "icons": {
    "64": "images/logo.png"
  },
  "manifest_version": 2,
  "name": "Materinstvo Filter",
  "permissions": [
    "declarativeContent",
    "tabs",
    "storage",
    "https://materinstvo-shop.ru/purchases/view/*"
  ],
  "version": "1.3"
}